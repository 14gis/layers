{
  "name": "14gis/layers",
  "description": "Geospatial Layer Core for 14gis Projects",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Gis14\\Layers\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.2",
    "symfony/yaml": "^7.0",
    "psr/http-server-middleware": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/container": "^2.0",
    "nyholm/psr7": "^1.8",
    "nyholm/psr7-server": "^1.1",
    "nikic/fast-route": "^1.3"
  },
  "scripts": {
    "build": "php bin/compile.php"
  },
  "require-dev": {
    "pestphp/pest": "^3.8"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
